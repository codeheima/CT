		<name>messageExist</name> 
		<tag-class>taiji.snappy.web.tags.MessageExistTag</tag-class> 
		<body-content>JSP</body-content> 														
	</tag>				
	
	<tag>
		<name>url</name> 
		<tag-class>taiji.snappy.web.tags.UrlTag</tag-class> 
		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>src</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>													
	</tag>
	
	<tag>
		<name>root</name> 
		<tag-class>taiji.snappy.web.tags.RootTag</tag-class> 
		<body-content>JSP</body-content> 															
	</tag>		
	
	<tag>
		<name>exception</name> 
		<tag-class>taiji.snappy.web.tags.ExceptionTag</tag-class> 
		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>																		
	</tag>	
	
	<tag>
		<name>iterator</name> 
		<tag-class>taiji.snappy.web.tags.IteratorTag</tag-class> 
		<!--tei-class>taiji.snappy.web.tags.IteratorTagExtraInfo</tei-class-->
		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>		
		<attribute>
			<name>property</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>																				
