		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>													
	</tag>	
	
	<tag>
		<name>error</name> 
		<tag-class>taiji.snappy.web.tags.ErrorTag</tag-class> 
		<body-content>JSP</body-content> 														
	</tag>	
	
