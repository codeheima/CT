		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>property</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>		
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>																								
	</tag>
	
	<tag>
		<name>value</name> 
		<tag-class>taiji.snappy.web.tags.ValueTag</tag-class> 
		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>equal</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>notequal</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>																			
	</tag>			
	
	<tag>
		<name>if</name> 
		<tag-class>taiji.snappy.web.tags.IFTag</tag-class> 
		<body-content>JSP</body-content> 		
		
		<attribute>
			<name>test</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>																								
	</tag>											
</taglib> 