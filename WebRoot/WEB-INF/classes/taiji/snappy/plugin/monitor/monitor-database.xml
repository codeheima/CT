<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE snappydb-configuration PUBLIC "-//SnappyDB/SnappyDB Configuration DTD 1.0//EN" "http://www.taiji.com.cn/snappydb-configuration-1.0.dtd">

<snappydb-configuration>
	<databases>
		<database name="ctdb">
			<tables>
				<table id="ctdb.resCurrent" name="RES_CURRENT" keygen="assign"/>
				<table id="ctdb.ctParam" name="ct_param" keygen="assign"/>
			</tables>
			<scripts>
				<script resource="taiji/snappy/plugin/monitor/dao/ResCurrent.tsd.xml"/>
				<script resource="taiji/snappy/plugin/monitor/dao/CtParam.tsd.xml"/>
			</scripts>
		</database>
	</databases>
</snappydb-configuration>
